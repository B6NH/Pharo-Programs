Object subclass: #FizzBuzzExample	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'SomeFizzBuzz'!!FizzBuzzExample methodsFor: 'accessing' stamp: 'ab 2/23/2019 20:46'!start	(1 to: 100) do:[:i|	|text|	(((i rem: 5)isZero) and: ((i rem:3)isZero))		ifTrue:[text := 'FizzBuzz']		ifFalse:[			((i rem:3) = 0)				ifTrue:[text := 'Fizz']				ifFalse:[					((i rem:5) = 0)						ifTrue:[text := 'Buzz']						ifFalse:[text := i asString]]].				Transcript show: text;cr]! !
